CREATE TABLE Global_Index_Table (
	Seq       INT NOT NULL AUTO_INCREMENT,
    Date      DATETIME NOT NULL,
	USD_KRW   DECIMAL(9, 4) NOT NULL,
    CNY_KRW   DECIMAL(9, 4) NOT NULL,
    HKD_KRW   DECIMAL(9, 4) NOT NULL,
    SGD_KRW   DECIMAL(9, 4) NOT NULL,
    BRKR      DECIMAL(9, 4) NOT NULL,
    BRUS      DECIMAL(9, 4) NOT NULL,
    KOSPI     DECIMAL(9, 4) NOT NULL,
    NASDAQ    DECIMAL(9, 4) NOT NULL,
    SPX       DECIMAL(9, 4) NOT NULL,
    DJI       DECIMAL(9, 4) NOT NULL, 
	PRIMARY KEY(seq)
);

SELECT    USD_KRW, CNY_KRW, HKD_KRW, SGD_KRW, BRKR, BRUS, KOSPI, NASDAQ, SPX, DJI
FROM      Global_Index_Table
ORDER BY  Date;
